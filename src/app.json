{
  "pages": [
    "pages/index/index",
    "pages/findProject/chat",
    "pages/findProject/result",
    "pages/findVenue/chat",
    "pages/findVenue/result",
    "pages/industryAnalysis/chat",
    "pages/industryAnalysis/result",
    "pages/relocEval/chat",
    "pages/relocEval/result",
    "pages/history/index",
    "pages/favorites/index",
    "pages/profile/index"
  ],
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#fff",
    "navigationBarTitleText": "招乎AI助手",
    "navigationBarTextStyle": "black",
    "navigationStyle": "custom"
  },
  "tabBar": {
    "custom": true,
    "color": "#999999",
    "selectedColor": "#1976D2",
    "backgroundColor": "#FFFFFF",
    "borderStyle": "white",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页"
      },
      {
        "pagePath": "pages/profile/index",
        "text": "我的"
      },
      {
        "pagePath": "pages/history/index",
        "text": "历史"
      },
      {
        "pagePath": "pages/favorites/index",
        "text": "收藏"
      }
    ]
  },
  "usingComponents": {
    "navigation-bar": "./components/common/navigation-bar/navigation-bar",
    "custom-tabbar": "./components/common/custom-tabbar/custom-tabbar"
  },
  "style": "v2",
  "renderer": "skyline",
  "rendererOptions": {
    "skyline": {
      "defaultDisplayBlock": true,
      "defaultContentBox": true,
      "tagNameStyleIsolation": "legacy",
      "disableABTest": true,
      "sdkVersionBegin": "3.0.0",
      "sdkVersionEnd": "15.255.255"
    }
  },
  "componentFramework": "glass-easel",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents",
  "networkTimeout": {
    "request": 300000,
    "connectSocket": 60000,
    "uploadFile": 60000,
    "downloadFile": 60000
  },
  "permission": {
    "scope.userLocation": {
      "desc": "您的位置信息将用于为您推荐附近的项目和载体"
    },
    "scope.subscribeMessage": {
      "desc": "订阅消息用于接收任务处理完成通知"
    }
  },
  "requiredPrivateInfos": [
    "getLocation"
  ]
}